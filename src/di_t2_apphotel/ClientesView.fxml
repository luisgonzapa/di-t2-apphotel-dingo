<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="rootAgendaView" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="di_t2_apphotel.ClientesViewController">
   <children>
      <VBox layoutY="-1.0" prefHeight="400.0" prefWidth="602.0" spacing="20.0">
         <children>
            <TableView fx:id="tableViewHotel" prefHeight="330.0" prefWidth="429.0">
              <columns>
                <TableColumn fx:id="columnDNI" prefWidth="92.0" text="DNI" />
                <TableColumn fx:id="columnNombre" prefWidth="88.0" text="Nombre" />
                <TableColumn fx:id="columnDireccion" prefWidth="122.0" text="Direccion" />
                <TableColumn fx:id="columnLocalidad" prefWidth="79.0" text="Localidad" />
                <TableColumn fx:id="columnProvincia" prefWidth="92.0" text="Provincia" />
                <TableColumn fx:id="columnTelefono" prefWidth="108.0" text="Telefono" />                 
              </columns>
            </TableView>
            <Button fx:id="volver" mnemonicParsing="false" onAction="#volver" text="Volver" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
